{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "ConsoleApp",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/ConsoleApps/ConsoleApp/bin/Debug/net8.0/ConsoleApp.dll",
            "stopAtEntry": false,
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart",
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "build"
        },
        {
            "name": "BlazorWebAssemblyProject",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/ASP.NET/BlazorWebAssemblyProject/bin/Debug/net8.0/BlazorWebAssemblyProject.dll",
            "stopAtEntry": false,
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart",
            "cwd": "${workspaceFolder}/ASP.NET/BlazorWebAssemblyProject",
            "preLaunchTask": "EF Migration"
        },
        {
            "name": "RazorProject",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/ASP.NET/RazorProject/bin/Debug/net8.0/RazorProject.dll",
            "stopAtEntry": false,
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart",
            "cwd": "${workspaceFolder}/ASP.NET/RazorProject",
            "preLaunchTask": "EF Migration"
        },
        {
            "name": "Launch RazorProject with Migrations",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "EF MigRazor",
            "postDebugTask": "Build and Run RazorProject",
            "program": "${workspaceFolder}/ASP.NET/RazorProject/bin/Debug/net8.0/RazorProject.dll",
            "args": [],
            "cwd": "${workspaceFolder}/ASP.NET/RazorProject",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {},
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
            "name": "Launch BlazorWebAssemblyProject with Migrations",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "EF MigBlazor",
            "postDebugTask": "Build BlazorWebAssemblyProject",
            "program": "${workspaceFolder}/ASP.NET/BlazorWebAssemblyProject/bin/Debug/net8.0/BlazorWebAssemblyProject.dll",
            "cwd": "${workspaceFolder}/ASP.NET/BlazorWebAssemblyProject",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {}
        },
        {
            "type": "node",
            "name": "vscode-jest-tests.v2",
            "request": "launch",
            "args": [
                "--runInBand",
                "--watchAll=false",
                "--testNamePattern",
                "${jest.testNamePattern}",
                "--runTestsByPath",
                "${jest.testFile}"
            ],
            "cwd": "d:\\FullStackPlus",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "stopOnEntry": false,
            "env": {},
            "runtimeExecutable": "d:\\FullStackPlus\\node.exe",
            "runtimeArgs": [
                "run",
            ],
            "program": "d:\\FullStackPlus\\node_modules\\jest\\bin\\jest.js"
        }
    ]
}